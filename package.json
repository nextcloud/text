		"@tiptap/vue-2": "^2.0.0-beta.69",
		"core-js": "^3.20.3",
		"entities": "^3.0.1",
		"escape-html": "^1.0.3",
		"highlight.js": "^10.7.2",
		"lowlight": "^2.4.1",
		"markdown-it": "^12.3.2",
		"markdown-it-task-lists": "^2.1.1",
