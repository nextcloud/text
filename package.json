    "markdown-it-front-matter": "^0.2.3",
    "markdown-it-image-figures": "^2.1.1",
    "mitt": "^3.0.0",
    "path-normalize": "^6.0.12",
    "proxy-polyfill": "^0.3.2",
    "slug": "^8.2.2",
